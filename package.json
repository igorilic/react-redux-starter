{
  "name": "redux-simple-starter",
  "version": "1.0.1",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:igorilic/react-redux-starter.git",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:dev lint:watch test:watch",
    "open:dev": "babel-node tools/devServer.js --hot",
    "lint": "esw webpack.config.*, src, tools --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "deploy": "surge ./dist"
  },
  "author": "Igor Ilic",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.18.0",
    "chai": "^4.0.2",
    "chai-jquery": "^2.0.0",
    "chalk": "^1.1.3",
    "cheerio": "^1.0.0-rc.1",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^11.0.0",
    "mocha": "^3.4.2",
    "nock": "^9.0.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.6.0",
    "redux-immutable-state-invariant": "^2.0.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "babel-preset-stage-1": "^6.1.18",
    "bootstrap": "^4.0.0-alpha.5",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
    "redux": "^3.0.4",
    "redux-thunk": "^2.2.0"
  }
}
